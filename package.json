{
  "name": "radio-color-system",
  "version": "0.1.7",
  "license": "MIT",
  "scripts": {
    "dev": "wireit",
    "build": "wireit",
    "preview": "wireit",
    "package": "wireit",
    "prepublishOnly": "wireit",
    "check": "wireit",
    "lint": "wireit",
    "clean": "wireit",
    "fix": "wireit",
    "test": "wireit",
    "doc": "wireit"
  },
  "wireit": {
    "vite:dev":{
      "command": "vite dev"
    },
    "vite:build":{
      "command": "vite build"
    },
    "vite:preview":{
      "command": "vite preview"
    },
    "doc:prepare": {
      "command": "jsdoc -c ./jsdoc.config.json -X > ./src/documentation.json"
    },
    "doc:clear":{
      "command": "sed -i '' '/RadioColorSystem/dist/package/d' ./src/documentation.json"
    },
    "doc:stash":{
      "command": "git stash && git checkout documentation && git stash pop"
    },
    "kit:sync":{
      "command": "svelte-kit sync"
    },
    "kit:check":{
      "command": "svelte-check --tsconfig ./tsconfig.json"
    },
    "kit:package":{
      "command": "svelte-package"
    },
    "npm:package":{
      "command": "npm package"
    },
    "gts:lint":{
      "command": "gts lint"
    },
    "gts:clean":{
      "command": "gts clean"
    },
    "gts:fix":{
      "command": "gts fix"
    },
    "jest":{
      "command": "jest"
    },
    "publint":{
      "command": "publint"
    },
    "package":{
      "dependencies":[
        "kit:sync",
        "kit:package",
        "publint"
      ]
    },
    "doc": {
      "dependencies":[
        "doc:prepare",
        "doc:clear",
        "doc:stash"
      ]
    },
    "check":{
      "dependencies":[
        "kit:sync",
        "kit:check"
      ]
    },
    "dev":{
      "dependencies":[
        "vite:dev"
      ]
    },
    "build":{
      "dependencies":[
        "vite:build",
        "npm:package"
      ]
    },
    "preview":{
      "dependencies":[
        "vite:preview"
      ]
    },
    "prepublishOnly":{
      "dependencies":[
        "npm:package"
      ]
    },
    "lint":{
      "dependencies":[
        "gts:lint"
      ]
    },
    "clean":{
      "dependencies":[
        "gts:clean"
      ]
    },
    "fix":{
      "dependencies":[
        "gts:fix"
      ]
    },
    "test":{
      "dependencies":[
        "jest"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "@types/css-tree": "^2.3.1",
    "@types/node": "^14.11.2",
    "babel-jest": "^29.5.0",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "wireit": "^0.9.5"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "css-tree": "^2.3.1"
  },
  "keywords": [
    "svelte",
    "color",
    "palette",
    "color-system"
  ]
}
